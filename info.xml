<?xml version="1.0"?>
<extension key="de.systopia.stoken" type="module">
  <file>stoken</file>
  <name>Additional Tokens</name>
  <description>Adds some new handy CiviCRM tokens</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.stoken</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.stoken</url>
    <url desc="Support">https://github.com/systopia/de.systopia.stoken/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate></releaseDate>
  <version>0.9-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>4.6</ver>
    <ver>4.7</ver>
    <ver>5.0</ver>
  </compatibility>
  <civix>
    <namespace>CRM/Stoken</namespace>
    <format>23.02.1</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
